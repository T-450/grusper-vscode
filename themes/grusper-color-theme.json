  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#7A7D8C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete",
        "keyword.operator.typeof",
        "keyword.operator.void",
        "keyword.operator.instanceof",
        "keyword.operator.of",
        "keyword.operator.in",
        "variable.language.this",
        "variable.language.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#FFFA99",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call entity.name.function",
        "support.function",
        "entity.name.method",
        "meta.method-call entity.name.function"
      ],
      "settings": {
        "foreground": "#99FFFA"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple"
      ],
      "settings": {
        "foreground": "#D1FF99"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.language",
        "constant.character",
        "variable.other.constant",
        "variable.other.enummember",
        "support.constant",
        "constant.other.caps"
      ],
      "settings": {
        "foreground": "#FA99FF"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Identifiers, types, classes",
      "scope": [
        "variable",
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "entity.name.tag",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Operators and punctuation",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.ternary",
        "keyword.operator.spread",
        "keyword.operator.rest",
        "keyword.operator.type.annotation",
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "punctuation.section",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#5E6370"
      }
    },
    {
      "name": "Escape characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#FA99FF"
      }
    },
    {
      "name": "Decorators and annotations",
      "scope": [
        "meta.decorator",
        "punctuation.decorator",
        "meta.decorator entity.name.function",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#FA99FF"
      }
    },
    {
      "name": "Template expression punctuation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#FFFA99"
      }
    },
    {
      "name": "Regex",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#D1FF99"
      }
    },
    {
      "name": "CSS property names",
      "scope": [
        "support.type.property-name",
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less"
      ],
      "settings": {
        "foreground": "#9BA0AA"
      }
    },
    {
      "name": "CSS property values",
      "scope": [
        "support.constant.property-value",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "CSS tags",
      "scope": [
        "entity.name.tag.css",
        "entity.name.tag.scss",
        "entity.name.tag.less"
      ],
      "settings": {
        "foreground": "#FFFA99",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS units and colors",
      "scope": [
        "keyword.other.unit",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#FA99FF"
      }
    },
    {
      "name": "CSS pseudo selectors",
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#99FFFA"
      }
    },
    {
      "name": "Markdown headings",
      "scope": [
        "markup.heading",
        "entity.name.section",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#FFFA99",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown links",
      "scope": [
        "markup.underline.link",
        "markup.underline.link.image"
      ],
      "settings": {
        "foreground": "#99FFFA"
      }
    },
    {
      "name": "Markdown code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block",
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#D1FF99"
      }
    },
    {
      "name": "Markdown list markers",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#FFFA99"
      }
    },
    {
      "name": "JSON property names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#9BA0AA"
      }
    },
    {
      "name": "YAML keys",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#99FFFA"
      }
    },
    {
      "name": "HTML/XML tags",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.xml",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "HTML/XML attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.xml",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "meta.template.expression",
        "meta.embedded.expression"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Shell variables",
      "scope": [
        "variable.other.normal.shell",
        "variable.other.special.shell",
        "variable.other.positional.shell"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Invalid / deprecated",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#E05C5C"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FA99FF"
      }
    }
  ]
}
